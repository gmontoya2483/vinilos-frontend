{
  "source": {
    "include": ["src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "node_modules/jsdoc/plugins/markdown",
    "node_modules/angular-jsdoc/common/plugins/ngdoc"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  }
}
